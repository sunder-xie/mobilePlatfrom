<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
	<head>
		<title>SAP ACCOUNT</title>
		<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
		#CSSM(["ext", "style"])
	</head>
	<body>
		<form method="post">
			<table width="100%" height="100%" border="0" cellpadding="0" cellspacing="8">
				<tr>
					<td valign="top">
						<table width="100%" border="0" cellspacing="0" cellpadding="0" >
							<tr>
								<td class="form_top_icon_01" >
									ERP帐号管理
								</td>
								<td class="title_bottom_line" noWrap align='right'>
									<a href="javascript:createSAPAccount();">
                                        <img src="$!{env.imgUrl}/image/actions/icon_add.gif" align="absMiddle" >创建ERP帐号 </img>
									</a>
									<a href="javascript:syncSAPAccount();">
                                        <img src="$!{env.imgUrl}/image/actions/icon_refresh.gif" align="absMiddle" >更新ERP帐号状态 </img>
									</a>
									<br>
								</td>
							</tr>
							<tr>
								<td colspan="1" class="home_title">
									查询条件
								</td>
							</tr>
							<tr>
								<td colspan="2" class="home_title_line">
									<table width="100%" border="0" cellpadding="0" cellspacing="1" class=outer>
    									<tr>
    										<td class="head" noWrap style="text-align: center">
    											EPR帐号
    										</td>
											<td class="even" noWrap>
												<input type='text' id='account' />
											</td>
    										<td class="head" noWrap style="text-align: center">
    											帐号是否占用
    										</td>
											<td class="even" noWrap>
												<input type='text' id='occupy' />
											</td>
    										<td class="head" noWrap style="text-align: center">
    											帐号是否有效
    										</td>
											<td class="even" noWrap>
												<input type='text' id='status' />
											</td>
											<td class="even" noWrap>
												<input type="button" onclick="javascript:search()" value="查询" />
											</td>
                                    	</tr>
									</table>
									<tr>
        								<td colspan="1" class="home_title">
        									查询列表
        								</td>
									</tr>
								</td>
							</tr>
						</table>
					</td>
				</tr>
			</table>
			<div class="cent_border">
				<div id="gridList" class="ext-grid-mask"></div>
			</div>
            <input type='hidden' id='sapAccount' name="sapAccount" />
		</form>
		<iframe id="hideFrame" name="hideFrame"	src="$!{env.imgUrl}/image/space.gif" style="height: 0; width: 0"></iframe>
    	#JSM(["common", "ext", "jquery"])
        #JS_ENV_VAL()
        <script type="text/javascript">
        var store;
		var pageSize = 20;
        </script>
        <script type="text/javascript" src="$!{env.imgUrl}/js/xplatform/sap/searchSAPAccount.js"></script>
	</body>
</html>